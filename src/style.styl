//         transform scale(0)

.preload.topbar
    opacity 0

.song-card
    width 100%
    position relative
    margin 0 auto
    display block
    padding-top 40px
    // height 172px
// .detailed .song-card
    // height 192px


.song-name
    position relative
    left 24px
    font-size 20px

.btn-play
    font-weight: 700
    position absolute
    top 37px
    right 16px
    height 24px
    width 74px
    background var(--primary-color-regular)
    color white
    border-radius 40px
    border none
    
.measure
    width 100%
    // height 88px
    display grid
    grid-template-rows 0px 88px;

.detailed .measure
    grid-template-rows 20px 88px;

// Simple time signatures
.measure.two-four
    grid-template-columns repeat(8, 1fr);
.measure.three-four
    grid-template-columns repeat(12, 1fr);
.measure.four-four
    grid-template-columns repeat(16, 1fr);
// Compound time signatures
.measure.six-eight
    grid-template-columns repeat(24, 1fr);
.measure.nine-eight
    grid-template-columns repeat(18, 1fr);
.measure.twelve-eight
    grid-template-columns repeat(24, 1fr);
// Complex time signatures
.measure.five-four
    grid-template-columns repeat(10, 1fr);
// Made up time signatures
.measure.seven-four
    grid-template-columns repeat(14, 1fr);
    
.one-bar
    grid-template-columns repeat(1, 1fr);
.two-bars
    grid-template-columns repeat(1, 1fr);
.three-bars
    grid-template-columns repeat(1, 1fr);
.four-bars
    grid-template-columns repeat(8, 1fr);



.notation-frame
    display grid
    position relative
    margin-top 10px
    width 100%
    background var(--notation-bg-color)
    .MuiButton-label
        padding 0 16px
        display grid
        grid-template-columns repeat(1, 1fr);
    +desktop()
        border var(--notation-frame-border)
    +mobile()
        border-radius 0
        border-top var(--notation-frame-border)
        border-bottom var(--notation-frame-border)
    &:hover
        cursor pointer
        background var(--notation-bg-hover-color)
    &.active
        background var(--notation-bg-active-color)
    &.active:after
        content: '';
        display: block;
        position: absolute;
        top: 0;
        bottom: 0;
        left: 0;
        right: 0;
        border: 2px solid var(--primary-color-regular)
        +desktop()
            border-radius: 5px;
        +tablet()
            border-radius: 0px;
            border-right none
            border-left none
    +tablet()
        border-radius 0
    +desktop()
        border-radius 10px
        
    .highlighted
        background: #FAF0F3;
        :after
            content: '';
            display: block;
            position: absolute;
            top: 0;
            bottom: 0;
            left: 0;
            right: 0;
            border-radius: 5px;
            border: 2px solid var(--primary-color-regular)





.notation-note
    position relative
    // height 88px

.notation-number
    font-family: 'Poppins', sans-serif;
    opacity 0
    text-align center
    color var(--notation-number)
    font-size: 12px
    margin-top 10px
    display inline-block
    width 100%

.active .notation-number
    color var(--notation-number-active)

.detailed .notation-number
    opacity 1

.notation-horizontal-line
    position absolute
    height 1px
    width 100%
    background var(--notation-line-color)
    top 50%
    align-items center
    align-self center

.notation-vertical-line
    position absolute
    height 50%
    width 4%
    top 25%
    left 48%
    background var(--notation-line-color)
    &.active
        background var(--primary-color-regular)




// .detailed .notation-frame
//     height 108px


.notation-note-circle
    position relative
    width 15px
    height 15px
    border-radius 50px
    background var(--text-color)
    margin 0 auto
    // box-shadow: 0 0 1px 0px white inset

.Dun
    top 66%
.Gun
    top 66%
.do
    top 42%
.go
    top 42%
.Ta
    top 18%
.Pa
    top 18%
.Ca
    top 18%
    background var(--notation-bg-color)
    border 2px solid var(--text-color)
    &::after
        content ""
        width 3px
        height 3px
        left 4px
        top 4px
        position absolute
        background var(--text-color)
        border-radius 50px
.dos
    top 42%
    background var(--notation-bg-color)
    border 2px dotted var(--text-color)

.gos
    top 42%
    background var(--notation-bg-color)
    border 2px dotted var(--text-color)
    // border 2px dashed var(--text-color)

.xx
    display none

.active .Ca
    background var(--notation-bg-color)

