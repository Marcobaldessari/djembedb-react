.topbar
    position fixed
    max-width 900px
    width 100%
    height 56px
    z-index 10
    background var(--topbar-color)
    backdrop-filter blur(10px)
    
    +desktop()
        top 0
    +mobile()
        bottom 0
        box-shadow: 0px -15px 35px rgba(52, 58, 67, 0.08), 0px -5px 15px rgba(0, 0, 0, 0.08), 0px 0px 2px rgba(0, 0, 0, 0.1);
        display grid
        grid-template-columns repeat(6, 1fr)
        grid-template-rows: 1fr;
    
    .btn-topbar
        +desktop()
            float right
            right 0
        +mobile()
            height 64px
        padding 0
        border 0
        border-radius 100%
        background transparent
        svg
            fill var(--note-color)



    .logo
        +mobile()
                display none


        display inline-block
        position relative
        &:hover
            cursor pointer

        .logotype
            display inline-block
            position absolute
            svg 
                fill var(--text-color)
                width 137px
            +mobile()
                display none

        .logomark-wrapper
            display inline-block
            top 0
            margin 0
            position relative

        .logomark
            position relative
            display grid
            grid-template-columns repeat(4,1fr)
            grid-column-gap 4% 
            background var(--primary-color-regular)
            +desktop()
                // top -21px
                padding 7px
                margin 16px 16px 16px 0
                width 46px
                height 30px
                border-radius 10px
            +tablet()
                padding 7px
                margin 16px
                width 46px
                height 30px
                border-radius 10px
            &:hover
                cursor pointer

        .Ca
            border none 
            &::after
                content: none

